{
  "name": "project-homefront-webapp",
  "version": "0.0.0",
  "scripts": {
    "install": "npm-run-all *:install",
    "start": "npm-run-all *:start",
    "build": "npm-run-all *:build",
    "test": "npm-run-all *:test",
    "lint": "npm-run-all *:lint",
    "e2e": "npm-run-all *:e2e",
    "web:install": "npm install --prefix ./webapp",
    "web:ng": "npm run ng ./webapp",
    "web:start": "npm run start --prefix ./webapp",
    "web:build": "npm run build --prefix ./webapp",
    "web:test": "npm run test --prefix ./webapp",
    "web:lint": "npm run lint --prefix ./webapp",
    "web:e2e": "npm run e2e --prefix ./webapp"
  },
  "private": true,
  "dependencies": {
    "@typescript-eslint/parser": "^3.2.0",
    "config": "^3.3.1",
    "dotenv": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5"
  }
}
