{
  "name": "project-homefront-webapp",
  "version": "0.0.0",
  "scripts": {
    "install": "npm-run-all *:install",
    "start": "npm-run-all -pl *:start",
    "build": "npm-run-all *:build",
    "test": "npm-run-all *:test",
    "lint": "npm-run-all *:lint",
    "e2e": "npm-run-all *:e2e",
    "api:install": "npm install --prefix ./api",
    "api:build": "npm run build --prefix ./api",
    "api:test": "npm run test --prefix ./api",
    "api:start": "npm run startDev --prefix ./api",
    "api:pretty": "npm run pretty --prefix ./api",
    "web:install": "npm install --prefix ./webapp",
    "web:ng": "npm run ng ./webapp",
    "web:start": "npm run start --prefix ./webapp",
    "web:build": "npm run build --prefix ./webapp",
    "web:test": "npm run test --prefix ./webapp",
    "web:lint": "npm run lint --prefix ./webapp",
    "web:e2e": "npm run e2e --prefix ./webapp"
  },
  "private": true,
  "dependencies": {
    "config": "^3.3.1",
    "dotenv": "^8.2.0",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
